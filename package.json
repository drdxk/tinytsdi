{
  "name": "tinytsdi-monorepo",
  "version": "0.0.0",
  "description": "Monorepo for tinytsdi TypeScript Dependency Injection library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drdxk/tinytsdi.git"
  },
  "license": "ISC",
  "author": "Dmytro Kossa",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "turbo run build",
    "check": "turbo run check",
    "dev": "turbo run dev",
    "e2e": "turbo run e2e",
    "e2e:dev": "turbo run e2e:dev",
    "fix": "turbo run fix",
    "fox": "turbo run fox",
    "lint": "turbo run lint",
    "test:run": "turbo run test:run",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "pub": "turbo run pub",
    "root:check": "pnpm run root:lint",
    "root:fix": "prettier --write \"*.md\" \"docs/**/*.md\"",
    "root:fox": "pnpm run root:fix && pnpm run root:check",
    "root:lint": "prettier --check \"*.md\" \"docs/**/*.md\""
  },
  "devDependencies": {
    "eslint": "catalog:dev",
    "prettier": "catalog:dev",
    "prettier-plugin-jsdoc": "1.8.0",
    "turbo": "2.8.3"
  },
  "packageManager": "pnpm@10.28.2",
  "keywords": [
    "typescript",
    "dependency-injection",
    "di",
    "ioc",
    "inversion-of-control"
  ]
}
