{
  "name": "tinytsdi",
  "version": "3.0.0-alpha.9",
  "description": "Minimalistic TypeScript Dependency Injection library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drdxk/tinytsdi.git"
  },
  "license": "ISC",
  "author": "Dmytro Kossa",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build:tsc": "rm -rf dist && tsc -p tsconfig.build.types.json && tsc -p tsconfig.build.esm.json && tsc-alias -p tsconfig.build.esm.json && tsc -p tsconfig.build.cjs.json && tsc-alias -p tsconfig.build.cjs.json && find dist/cjs -name '*.js' -exec mv {} {}.cjs \\;",
    "build": "rm -rf dist && vite build",
    "check": "pnpm run typecheck && pnpm run lint && pnpm run test:run",
    "fix": "prettier --write src && eslint --fix",
    "fox": "pnpm run fix && pnpm run check",
    "lint": "eslint && prettier --check src",
    "test:coverage": "vitest run --coverage",
    "test:run": "vitest run",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "pub": "pnpm build && pnpm publish"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/node": "^24.5.1",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^58.1.1",
    "eslint-plugin-tsdoc": "^0.4.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-jsdoc": "^1.3.3",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.17.0+sha512.fce8a3dd29a4ed2ec566fb53efbb04d8c44a0f05bc6f24a73046910fb9c3ce7afa35a0980500668fa3573345bd644644fa98338fa168235c80f4aa17aa17fbef",
  "keywords": [
    "typescript",
    "dependency-injection",
    "di",
    "ioc",
    "inversion-of-control"
  ]
}
