{
  "name": "tinytsdi",
  "version": "3.0.0-alpha.2",
  "description": "Minimalistic TypeScript Dependency Injection library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drdxk/tinytsdi.git"
  },
  "license": "ISC",
  "author": "Dmytro Kossa",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build:tsc": "rm -rf dist && tsc -p tsconfig.build.types.json && tsc -p tsconfig.build.esm.json && tsc-alias -p tsconfig.build.esm.json && tsc -p tsconfig.build.cjs.json && tsc-alias -p tsconfig.build.cjs.json && find dist/cjs -name '*.js' -exec mv {} {}.cjs \\;",
    "build": "rm -rf dist && vite build",
    "check": "pnpm run typecheck && pnpm run lint && pnpm run test:run",
    "fix": "prettier --write src && eslint --fix",
    "fox": "pnpm run fix && pnpm run check",
    "lint": "eslint && prettier --check src",
    "test:coverage": "vitest run --coverage",
    "test:run": "vitest run",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "pub": "pnpm build && pnpm publish"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^54.4.0",
    "eslint-plugin-tsdoc": "^0.4.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-jsdoc": "^1.3.3",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2",
    "vite": "^7.1.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67",
  "keywords": [
    "typescript",
    "dependency-injection",
    "di",
    "ioc",
    "inversion-of-control"
  ]
}
