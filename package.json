{
  "name": "tinytsdi-monorepo",
  "version": "0.0.0",
  "description": "Monorepo for tinytsdi TypeScript Dependency Injection library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drdxk/tinytsdi.git"
  },
  "license": "ISC",
  "author": "Dmytro Kossa",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "turbo run build",
    "check": "turbo run check",
    "dev": "turbo run dev",
    "fix": "turbo run fix",
    "fox": "turbo run fox",
    "lint": "turbo run lint",
    "test:run": "turbo run test:run",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "pub": "turbo run pub",
    "root:check": "pnpm run root:lint",
    "root:fix": "prettier --write \"*.md\" \"docs/**/*.md\"",
    "root:fox": "pnpm run root:fix && pnpm run root:check",
    "root:lint": "prettier --check \"*.md\" \"docs/**/*.md\""
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "prettier-plugin-jsdoc": "^1.3.3",
    "turbo": "^2.5.6"
  },
  "packageManager": "pnpm@10.17.0+sha512.fce8a3dd29a4ed2ec566fb53efbb04d8c44a0f05bc6f24a73046910fb9c3ce7afa35a0980500668fa3573345bd644644fa98338fa168235c80f4aa17aa17fbef",
  "keywords": [
    "typescript",
    "dependency-injection",
    "di",
    "ioc",
    "inversion-of-control"
  ]
}
